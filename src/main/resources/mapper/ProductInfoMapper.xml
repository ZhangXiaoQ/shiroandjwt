<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cheetah.shiroandjwt.mapper.ProductInfoMapper">

    <resultMap id="BaseResultMap" type="com.cheetah.shiroandjwt.entity.ProductInfo">
        <id column="id" jdbcType="BIGINT" property="id" />
        <result column="name" jdbcType="VARCHAR" property="name" />
        <result column="price" jdbcType="DOUBLE" property="price" />
        <result column="create_date" jdbcType="TIMESTAMP" property="createDate" />
        <result column="update_date" jdbcType="TIMESTAMP" property="updateDate" />
    </resultMap>
    <sql id="Base_Column_List">
    id, name, price, create_date, update_date
    </sql>

    <select id="getProductInfoList" resultType="com.cheetah.shiroandjwt.entity.ProductInfo">
        SELECT * from product_info
    </select>

</mapper>